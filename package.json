{
  "name": "music-play",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.4.1",
    "disconnect": "1.2.1",
    "moment": "2.22.2",
    "music-metadata": "3.1.4",
    "nedb": "1.8.0",
    "node-notifier": "5.3.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-redux": "^5.1.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.5",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=images/icon.jpg --prune=true --out=release-builds",
    "package-win": "electron-packager . music-player --overwrite --platform win32 --arch x64 --out release-builds/",
    "package-linux": "electron-packager . music-player --overwrite --platform=linux --arch=x64 --icon=build/images/icon.jpg --out=release-builds"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^3.0.4",
    "electron-packager": "^12.2.0"
  }
}
